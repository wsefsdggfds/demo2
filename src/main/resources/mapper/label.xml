<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.shopping.label.mapper.LabelMapper">

    <!--商城 -->
    <!--商品管理 -->

    <!--添加标签 -->
    <insert id="insertLabel">
        insert into label(id, colour, name, number) value (#{id},#{colour},#{name},#{number})
    </insert>
    <delete id="delete">
        delete from label where id=#{id};

    </delete>
</mapper>